    "children": ["image#footerccs", "image#footercomprasegura", "image#footervtex", "image#footerpropulsow"],
    "props": {
      "blockClass": "credits"
    }
  },
  "image#footerccs": {
    "props": {
      "src": "/arquivos/ccs.svg",
      "blockClass": "footer",
      "width": 44,
      "height": 61.94
    }
  },

