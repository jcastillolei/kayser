{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#footer-1-desktop",
      "flex-layout.row#footer-2-desktop",
      "flex-layout.row#footer-3-desktop"
    ]
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "newsletter",
      "footer-spacer",
      "vtex.menu@2.x:menu#info-kayser",
      "footer-spacer",
      "vtex.menu@2.x:menu#servicio-cliente",
      "footer-spacer",
      "vtex.menu@2.x:menu#rrss",
      "footer-spacer",
      "vtex.menu@2.x:menu#catalogo"
    ],
    "props": {
      "blockClass": "menu-footer",
      "paddingTop": 0,
      "paddingBottom": 0
    }
  },
  "footer-spacer":{
    "props": {
      "width": "7%"
    }
  },
  "newsletter":{
    "props":{
      "label": "Suscríbete al Newsletter",
      "placeholder": "Ingrese su e-mail",
      "submit": "Enviar"
    }
  },

  "flex-layout.row#footer-2-desktop": {
    "children": ["vtex.menu@2.x:menu#busquedas-seo"],
    "props": {
      "blockClass": "busquedas-footer"
    }
  },
  "vtex.menu@2.x:menu#busquedas-seo":{
    "props": {
      "orientation": "horizontal",
      "blockClass": "busquedas-footer"
    },
    "children": [
      "menu-item#calzon-maternal-busqueda",
