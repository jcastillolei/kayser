      "menu-item#boxer-busqueda",
      "menu-item#colaless-busqueda",
      "menu-item#pijama-mujer-busqueda",
      "menu-item#pantaletas-mujer-busqueda",
      "menu-item#sostenes-mujer-busqueda",
      "menu-item#calzones-ninas-busqueda",
      "menu-item#calcetines-hombre-busqueda",
      "menu-item#ropa-interior-kids-hombre-busqueda",
      "menu-item#camisola-busqueda"
    ]
  },
  "menu-item#calzon-maternal-busqueda": {
    "props": {
      "id": "menu-item-busqueda",
      "type": "custom",
      "iconId": null,
